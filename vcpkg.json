{
  "name": "cpp-project-template",
  "version-semver": "1.1.0",
  "description": "Based on cmake-init but adapted to our needs and preferences",
  "homepage": "https://github.com/fantana21/CppProjectTemplate",
  "dependencies": [
    {
      "name": "fmt",
      "version>=": "10.2.1#1"
    },
    {
      "name": "namedtype",
      "version>=": "1.1.0-dev-1#1"
    },
    {
      "name": "quantnd",
      "version>=": "0.1.0"
    }
  ],
  "default-features": [],
  "features": {
    "test": {
      "description": "Dependencies for testing",
      "dependencies": [
        {
          "name": "doctest",
          "version>=": "2.4.11"
        }
      ]
    }
  },
  "vcpkg-configuration": {
    "default-registry": {
      "kind": "builtin",
      "baseline": "46e4c4c78c347ded6add526b0c2bb66db35d4710"
    },
    "registries": [
      {
        "kind": "git",
        "baseline": "765752b6d9c24592a726641202c2cc23f2b17103",
        "repository": "https://github.com/fantana21/vcpkg-registry",
        "packages": [
          "namedtype",
          "quantnd"
        ]
      }
    ]
  }
}
